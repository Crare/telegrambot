  "bugs": {
    "url": "https://github.com/crare/telegrambot/issues"
  },
  "homepage": "https://github.com/crare/telegrambot#readme",
  "dependencies": {
    "commander": "^2.18.0",
    "python-shell": "^0.5.0",
    "telegraf": "^3.24.0",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "mocha": "^10.2.0"
  }
}
